	"version": "0.2.0",
	"configurations": [
		{
			"name": "debug generate-payouts",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "main.go",
			"args": [
				"--log-level=trace",
				"generate-payouts",
				"--skip-balance-check"
			],
		},
		{
			"name": "debug pay",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "main.go",
			"args": [
				"--log-level=trace",
				"--pay-only-address-prefix=KT",
				"pay",
				"--cycle=623",
				"--confirm",
				"--dry-run"
			],
		},
		{
			"name": "debug continual",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "main.go",
			"args": [
				"--log-level=trace",
				"continual",
				"--cycle=620"
			],
		},
		{
			"name": "debug pay-date-range",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "main.go",
			"args": [
				"--log-level=trace",
				"pay-date-range",
				"--start-date",
				"2024-02-01",
				"--end-date",
				"2024-02-29"
			]
		}
	]
}